{
	"formatter": {
		"indentStyle": "space",
		"indentWidth": 2,
		"lineWidth": 80,
		"lineEnding": "lf"
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noDebugger": "off",
				"noConsoleLog": "off",
				"noAssignInExpressions": "off"
			},
			"style": {
				"noShoutyConstants": "warn",
				"useNamingConvention": "warn",
				"noCommaOperator": "off",
				"noParameterAssign": "off"
			}
		}
	}
}
